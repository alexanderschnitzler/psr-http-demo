{
    "name": "schnitzler/psr-http-demo",
    "description": "Small demo showing how to use PSR's to create an http client independent api client",
    "license": "MIT",
    "require": {
        "psr/http-client": "^1.0",
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.5",
        "symfony/http-client": "^5.0",
        "ricardofiorani/guzzle-psr18-adapter": "^1.0",
        "psr/http-factory": "^1.0",
        "nyholm/psr7": "^1.2",
        "php-http/httplug": "^2.1",
        "php-http/curl-client": "^2.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
